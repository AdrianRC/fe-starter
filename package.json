{
	"name": "frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "pnpm run compile && vite build",
		"preview": "vite preview",
		"test": "pnpm run test:integration && pnpm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"compile": "houdini pull-schema && houdini generate && paraglide-js compile --project ./project.inlang.json",
		"postinstall": "svelte-kit sync && pnpm run compile"
	},
	"devDependencies": {
		"@playwright/test": "^1.40.0",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/kit": "^1.27.6",
		"@types/cookie": "^0.5.4",
		"@types/three": "^0.158.3",
		"@typescript-eslint/eslint-plugin": "^6.12.0",
		"@typescript-eslint/parser": "^6.12.0",
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.35.1",
		"houdini": "^1.2.33",
		"houdini-svelte": "^1.2.33",
		"postcss": "^8.4.31",
		"postcss-load-config": "^4.0.2",
		"postcss-preset-env": "^9.3.0",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.2",
		"svelte-preprocess": "^5.1.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.2",
		"vite": "^4.5.0",
		"vitest": "^0.34.6"
	},
	"type": "module",
	"dependencies": {
		"@acab/reset.css": "^0.8.0",
		"@datocms/svelte": "^1.2.0",
		"@inlang/paraglide-js": "1.0.0-prerelease.11",
		"@theatre/core": "^0.7.0",
		"@theatre/studio": "^0.7.0",
		"@threlte/core": "^6.1.1",
		"@threlte/extras": "^7.5.1",
		"@threlte/theatre": "^2.1.3",
		"datocms-structured-text-utils": "^2.0.4",
		"gsap": "^3.12.2",
		"three": "^0.158.0"
	},
	"browserslist": "> 0.5%, last 2 Chrome major versions, last 2 Firefox major versions, last 2 Safari major versions, last 2 iOS major versions, not dead"
}
